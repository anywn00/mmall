{{#list}}
<li class="p-list-item">
	<div class="p-img-con">
		<a href="./detail.html?productId={{id}}" class="link" target="_blank">
			<img src="{{imageHost}}{{mainImage}}" alt="{{name}}" class="p-img">
		</a>
	</div>
	<div class="p-price-con">
		<span>￥{{price}}</span>
	</div>
	<div class="p-name-con">
		<a href="./detail.html?productId={{id}}" title="{{name}}" target="_blank" class="link-text">{{name}}</a>
	</div>
</li>
{{/list}}
{{^list}}
<p class="err-tip">很抱歉，实在找不到您要的商品。</p>
{{/list}}
	